{
  "$schema": "https://raw.githubusercontent.com/grafana/grafana/master/docs/sources/developers/plugins/plugin.schema.json",
  "type": "datasource",
  "name": "MongoDB Atlas Metrics",
  "id": "fernandoescolar-mongodbatlas-datasource",
  "metrics": true,
  "backend": true,
  "alerting": true,
  "executable": "gpx_mongodbatlas-datasource",
  "info": {
    "description": "Display MongoDB Atlas metrics inside of Grafana",
    "author": {
      "name": "Fernando Escolar",
      "url": "https://github.com/fernandoescolar"
    },
    "keywords": ["mongodb", "atlas", "metrics"],
    "logos": {
      "small": "img/logo.svg",
      "large": "img/logo.svg"
    },
    "links": [
      {
        "name": "GitHub",
        "url": "https://github.com/fernandoescolar/grafana-mongodb-atlas-datasource"
      },
      {
        "name": "MIT License",
        "url": "https://github.com/fernandoescolar/grafana-mongodb-atlas-datasource/blob/master/LICENSE"
      },
      {
        "name": "Metrics Docs",
        "url": "https://docs.atlas.mongodb.com/api/"
      }
    ],
    "screenshots": [
      {
        "name": "Setup",
        "path": "img/screenshots/datasource_setup.png"
      },
      {
        "name": "Query Example",
        "path": "img/screenshots/query_example.png"
      },
      {
        "name": "Query Options",
        "path": "img/screenshots/query_setup.png"
      }
    ],
    "version": "%VERSION%",
    "updated": "%TODAY%"
  },
  "dependencies": {
    "grafanaDependency": ">=7.0.0",
    "plugins": []
  }
}